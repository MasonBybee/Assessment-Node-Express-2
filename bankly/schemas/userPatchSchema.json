{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "title": "Root Schema",
  "type": "object",
  "default": {},
  "required": [],
  "additionalProperties": false,
  "properties": {
    "first_name": {
      "title": "The first_name Schema",
      "type": "string",
      "default": "",
      "examples": ["Mason"]
    },
    "last_name": {
      "title": "The last_name Schema",
      "type": "string",
      "default": "",
      "examples": ["Bybee"]
    },
    "phone": {
      "title": "The phone Schema",
      "type": "string",
      "default": "",
      "examples": ["8443030932"]
    },
    "email": {
      "title": "The email Schema",
      "type": "string",
      "default": "",
      "examples": ["masonbybee@gmail.com"]
    }
  },
  "examples": [
    {
      "first_name": "Mason",
      "last_name": "Bybee",
      "phone": "8443030932",
      "email": "masonbybee@gmail.com"
    }
  ]
}
